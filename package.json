{
  "name": "@willsoto/nestjs-prometheus",
  "description": "NestJS module for Prometheus",
  "license": "Apache-2.0",
  "author": "Will Soto <willsoto@users.noreply.github.com> (https://github.com/willsoto)",
  "repository": "git://github.com/willsoto/nestjs-prometheus.git",
  "version": "0.1.0",
  "main": "dist/nestjs-prometheus.umd.js",
  "module": "dist/nestjs-prometheus.esm.js",
  "files": [
    "dist",
    "src",
    "typings"
  ],
  "scripts": {
    "prebuild": "yarn clean",
    "build": "rollup --config rollup.config.js",
    "postbuild": "yarn typings",
    "clean": "rimraf dist typings coverage .nyc_output docs",
    "docs": "compodoc -p tsconfig.json --output docs --theme stripe -n 'NestJS Prometheus'",
    "lint": "eslint src test ad-hocs --ext ts --fix",
    "prerelease": "yarn build",
    "release": "standard-version --sign",
    "test": "mocha",
    "pretest:cov": "rimraf coverage .nyc_output",
    "test:cov": "nyc mocha",
    "test:watch": "mocha --watch",
    "pretypings": "rimraf typings",
    "typings": "tsc --project tsconfig.build.json --declaration --declarationDir typings --emitDeclarationOnly"
  },
  "typings": "./typings/index.d.ts",
  "peerDependencies": {
    "prom-client": "^12.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@compodoc/compodoc": "1.1.11",
    "@istanbuljs/nyc-config-typescript": "1.0.1",
    "@nestjs/cli": "6.14.2",
    "@nestjs/common": "6.11.11",
    "@nestjs/core": "6.11.11",
    "@nestjs/platform-express": "6.11.9",
    "@nestjs/schematics": "6.9.4",
    "@nestjs/testing": "6.11.9",
    "@types/chai": "4.2.10",
    "@types/chai-as-promised": "7.1.2",
    "@types/eslint": "6.1.8",
    "@types/express": "4.17.2",
    "@types/mocha": "7.0.1",
    "@types/node": "13.7.7",
    "@types/prettier": "1.19.0",
    "@types/prettier-package-json": "2.1.0",
    "@types/rimraf": "2.0.3",
    "@types/sinon-chai": "3.2.3",
    "@types/source-map-support": "0.5.1",
    "@types/supertest": "2.0.8",
    "@typescript-eslint/eslint-plugin": "2.22.0",
    "@typescript-eslint/parser": "2.22.0",
    "@willsoto/eslint-config-base": "2.0.0",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.0",
    "husky": "4.2.3",
    "lint-staged": "10.0.8",
    "markdown-toc": "1.2.0",
    "mocha": "7.1.0",
    "nyc": "15.0.0",
    "prettier": "1.19.1",
    "prettier-package-json": "2.1.3",
    "prom-client": "12.0.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rollup": "1.32.0",
    "rollup-plugin-eslint": "7.0.0",
    "rollup-plugin-filesize": "6.2.1",
    "rollup-plugin-typescript": "1.0.1",
    "rxjs": "6.5.4",
    "sinon-chai": "3.5.0",
    "source-map-support": "0.5.16",
    "standard-version": "7.1.0",
    "supertest": "4.0.2",
    "ts-node": "8.6.2",
    "tsconfig-paths": "3.9.0",
    "tslib": "1.11.1",
    "typescript": "3.8.3"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{md,yaml,yml}": [
      "prettier --write"
    ],
    "*.ts": [
      "yarn lint",
      "prettier --write"
    ],
    "package.json": [
      "prettier-package-json --write"
    ],
    "README.md": [
      "markdown-toc -i",
      "prettier --write"
    ]
  },
  "mocha": {
    "extension": [
      "ts"
    ],
    "file": "./ad-hocs/mocha/plugins.ts",
    "require": "ts-node/register/transpile-only,tsconfig-paths/register,source-map-support/register",
    "watch-files": [
      "src/**/*.ts",
      "test/**/*.ts"
    ],
    "recursive": true,
    "sort": true
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true,
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "src/**/*.spec.ts"
    ],
    "require": [
      "ts-node/register/transpile-only",
      "source-map-support/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "check-coverage": true,
    "lines": 90,
    "statements": 90,
    "functions": 90,
    "branches": 80,
    "watermarks": {
      "lines": [
        80,
        90
      ],
      "functions": [
        80,
        90
      ],
      "branches": [
        80,
        90
      ],
      "statements": [
        80,
        90
      ]
    }
  },
  "volta": {
    "node": "12.16.1",
    "yarn": "1.22.0"
  }
}
