{
  "name": "@willsoto/nestjs-prometheus",
  "description": "",
  "license": "Apache-2.0",
  "author": {
    "name": "Will Soto",
    "email": "willsoto@users.noreply.github.com",
    "url": "https://github.com/willsoto"
  },
  "repository": "git://github.com/willsoto/nestjs-prometheus.git",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "version": "0.0.1",
  "main": "dist/nestjs-prometheus.umd.js",
  "module": "dist/nestjs-prometheus.esm.js",
  "files": [
    "dist",
    "src",
    "typings"
  ],
  "scripts": {
    "docs": "compodoc -p tsconfig.json --output docs --theme stripe -n 'NestJS Prometheus'",
    "prebuild": "yarn clean",
    "build": "rollup --config rollup.config.js",
    "postbuild": "yarn typings",
    "clean": "rimraf dist typings coverage .nyc_output docs",
    "lint": "eslint src test ad-hocs --ext ts --fix",
    "prerelease": "yarn build",
    "release": "standard-version --sign",
    "test": "mocha",
    "pretest:cov": "rimraf coverage .nyc_output",
    "test:cov": "nyc mocha",
    "test:watch": "mocha --watch",
    "pretypings": "rimraf typings",
    "typings": "tsc --project tsconfig.build.json --declaration --declarationDir typings --emitDeclarationOnly"
  },
  "typings": "./typings/index.d.ts",
  "peerDependencies": {
    "prom-client": "^11.5.3"
  },
  "devDependencies": {
    "@commitlint/cli": "8.2.0",
    "@commitlint/config-conventional": "8.2.0",
    "@compodoc/compodoc": "1.1.11",
    "@istanbuljs/nyc-config-typescript": "0.1.3",
    "@nestjs/cli": "6.11.3",
    "@nestjs/common": "6.9.0",
    "@nestjs/core": "6.9.0",
    "@nestjs/platform-express": "6.9.0",
    "@nestjs/schematics": "6.7.0",
    "@nestjs/testing": "6.9.0",
    "@types/chai": "4.2.0",
    "@types/chai-as-promised": "7.1.1",
    "@types/eslint": "6.1.3",
    "@types/express": "4.17.2",
    "@types/mocha": "5.2.7",
    "@types/node": "12.12.7",
    "@types/prettier": "1.18.3",
    "@types/prettier-package-json": "2.1.0",
    "@types/rimraf": "2.0.3",
    "@types/sinon-chai": "3.2.3",
    "@types/source-map-support": "0.5.0",
    "@types/supertest": "2.0.8",
    "@typescript-eslint/eslint-plugin": "2.6.1",
    "@typescript-eslint/parser": "2.6.1",
    "@willsoto/eslint-config-base": "2.0.0",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "eslint": "6.6.0",
    "eslint-config-prettier": "6.5.0",
    "husky": "3.0.9",
    "lint-staged": "9.4.2",
    "markdown-toc": "1.2.0",
    "mocha": "6.2.2",
    "nyc": "14.1.1",
    "prettier": "1.19.1",
    "prettier-package-json": "2.1.0",
    "prom-client": "11.5.3",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.0",
    "rollup": "1.26.3",
    "rollup-plugin-eslint": "7.0.0",
    "rollup-plugin-filesize": "6.2.1",
    "rollup-plugin-typescript": "1.0.1",
    "rxjs": "6.5.3",
    "sinon-chai": "3.3.0",
    "source-map-support": "0.5.16",
    "standard-version": "7.0.1",
    "supertest": "4.0.2",
    "ts-node": "8.4.1",
    "tsconfig-paths": "3.9.0",
    "tslib": "1.10.0",
    "typescript": "3.7.2"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{md,yaml,yml}": [
      "prettier --write",
      "git add"
    ],
    "*.ts": [
      "yarn lint",
      "prettier --write",
      "git add"
    ],
    "package.json": [
      "prettier-package-json --write",
      "git add"
    ],
    "README.md": [
      "markdown-toc -i",
      "git add"
    ]
  },
  "mocha": {
    "watch-extensions": "ts",
    "file": "./ad-hocs/mocha/plugins.ts",
    "require": "ts-node/register/transpile-only,tsconfig-paths/register,source-map-support/register",
    "opts": false,
    "watch-files": [
      "src/**/*.ts",
      "test/**/*.ts"
    ],
    "recursive": true,
    "sort": true
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true,
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "src/**/*.spec.ts"
    ],
    "require": [
      "ts-node/register/transpile-only",
      "source-map-support/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "check-coverage": true,
    "lines": 90,
    "statements": 90,
    "functions": 90,
    "branches": 80,
    "watermarks": {
      "lines": [
        80,
        90
      ],
      "functions": [
        80,
        90
      ],
      "branches": [
        80,
        90
      ],
      "statements": [
        80,
        90
      ]
    }
  },
  "volta": {
    "node": "12.12.0",
    "yarn": "1.19.1"
  }
}
